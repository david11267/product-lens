generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model User {
  id               String            @id @default(uuid())
  email            String            @unique
  name             String?
  aiTokens         Int               @default(5)
  identifyRequests IdentifyRequest[]
}

model IdentifyRequest {
  id          String  @id @default(uuid())
  description String?
  images      Json
  userId      String
  user        User    @relation(fields: [userId], references: [id])
}


